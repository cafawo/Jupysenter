{
  "name": "jupysenter",
  "displayName": "Jupysenter",
  "description": "Jupysenter enhances the presentation of Jupyter Notebooks in VS Code by enabling smooth navigation using PageUp/PageDown keys â€” ideal for teaching or when using a wireless presenter.",
  "version": "1.0.4",
  "author": "Fabian Woebbeking",
  "publisher": "woebbeking",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Notebooks",
    "Education"
  ],
  "keywords": [
    "jupyter",
    "notebook",
    "notebooks",
    "presentation",
    "teaching",
    "education",
    "classroom",
    "slide",
    "pageup",
    "pagedown",
    "navigation",
    "cell navigation",
    "vs code notebooks"
  ],
  "activationEvents": [
    "onNotebookOpen"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.focusNextCellAndScroll",
        "title": "Focus Next Cell and Scroll"
      },
      {
        "command": "extension.focusPreviousCellAndScroll",
        "title": "Focus Previous Cell and Scroll"
      }
    ],
    "keybindings": [
      {
        "key": "pagedown",
        "command": "extension.focusNextCellAndScroll",
        "when": "resourceExtname == .ipynb"
      },
      {
        "key": "pageup",
        "command": "extension.focusPreviousCellAndScroll",
        "when": "resourceExtname == .ipynb"
      },
      {
        "mac": "right",
        "command": "extension.focusNextCellAndScroll",
        "when": "notebookEditorFocused && !notebookCellEditorFocused && !inputFocus && resourceExtname == .ipynb"
      },
      {
        "mac": "left",
        "command": "extension.focusPreviousCellAndScroll",
        "when": "notebookEditorFocused && !notebookCellEditorFocused && !inputFocus && resourceExtname == .ipynb"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.1",
    "typescript": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cafawo/Jupysenter.git"
  },
  "bugs": {
    "url": "https://github.com/cafawo/Jupysenter/issues"
  },
  "homepage": "https://github.com/cafawo/Jupysenter#readme"
}
